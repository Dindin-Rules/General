//----------------------------------------------
// This section checks for the running variables
//----------------------------------------------
//
headmsg '1.3 (around line 440) Varables' 38
if not @inlist! 'setup' 'Variablesfound'
  headmsg '1.3.1'
  //
  headmsg '1.3.2'
  sysmsg "Setting up Variables" 88
  pause 333
  @useobject 'backpack'
  pause 333
  //
  // Setting up Runes
  //
  if not findalias 'birdrune'
    headmsg '1.3.4'
    sysmsg "Select rune for bird killing" 88
    promptalias 'birdrune'
  endif
  if not findalias 'Regvendorrune'
    headmsg '1.3.5'
    sysmsg "Select rune your reg vendor" 88
    promptalias 'Regvendorrune'
  endif
  if not findalias 'Regvendorrune2'
    headmsg '1.3.6'
    sysmsg "Select rune your reg vendor" 88
    promptalias 'Regvendorrune2'
  endif
  if not findalias 'zoorune'
    headmsg '1.3.6'
    sysmsg "Select rune your Zoo Rune" 88
    promptalias 'zoorune'
  endif
  //
  // Setting up Runes
  //
  //
  // Find the Training Pet
  //
  headmsg '1.3.12'
  if not findalias 'trainerpet'
    headmsg '1.3.13'
    sysmsg "Select your account training target" 88
    promptalias 'trainerpet'
  endif
  //
  // Find the Training Pet
  //
  //
  // Find the Reg Vendor One
  //
  headmsg '1.3.14'
  if not findalias 'Regvendor'
    headmsg '1.3.15'
    pause 333
    headmsg 'Recalling to your RegVendor1'
    @canceltarget
    cast "Recall"
    waitfortarget 5000
    target! 'regvendorrune'
    pause 2000
    promptalias 'Regvendor'
  endif
  //
  // Find the Reg Vendor One
  //
  //
  // Find the Reg Vendor Two
  //
  headmsg '1.3.14'
  if not findalias 'Regvendor2'
    headmsg '1.3.15'
    pause 333
    headmsg 'Recalling to your RegVendor2'
    @canceltarget
    cast "Recall"
    waitfortarget 5000
    target! 'regvendorrune2'
    pause 2000
    promptalias 'Regvendor2'
  endif
  //
  // Find the Reg Vendor Two
  //
  headmsg '1.3.20'
  //
  // Double testing the variables to make sure we have everything.
  //
  headmsg '1.3.21'
  if findalias 'birdrune'
    if findalias 'Regvendorrune'
      if findalias 'Regvendorrune2'
        if findalias 'trainerpet'
          if findalias 'Regvendor'
            if findalias 'RegVendor2'
              headmsg '1.3.22'
              pause 333
              @clearlist 'setup'
              pushlist 'setup' 'Variablesfound'
            endif
          endif
        endif
      endif
    endif
  endif
  //
  // Double testing the variables to make sure we have everything.
  //
endif
//
//----------------------------------------------
// This section checks for the running variables
//----------------------------------------------
//
//
//----------------------------------------------
// This section is the running script
//----------------------------------------------
//
while not dead
  //
  //----------------------------------------------
  // This section is the running script
  //----------------------------------------------
  //
  //
  //----------------------------------------------
  // This section Buys Regs
  //----------------------------------------------
  //
  //
  if @inlist! 'Regcountpassattempts' 'first'
    //  @ Regcountpassattempts
  endif
  headmsg '1.8.2.1' 38
  if counttype '0xf86' 'any' 'backpack' < 10
    headmsg '1.8.2' 38
    pushlist 'Regcount' 'buyregs-Mandrakeroot'
  endif
  //
  headmsg '1.8.3.1' 38
  if counttype '0xf84' 'any' 'backpack' < 10
    headmsg '1.8.3' 38
    pushlist 'Regcount' 'buyregs-Garlic'
  endif
  //
  headmsg '1.8.4.1' 38
  if counttype '0xf85' 'any' 'backpack' < 10
    headmsg '1.8.4' 38
    pushlist 'Regcount' 'buyregs-Ginseng'
  endif
  //
  headmsg '1.8.5.1' 38
  if counttype '0xf8d' 'any' 'backpack' < 10
    headmsg '1.8.5' 38
    pushlist 'Regcount' 'buyregs-Spiders Sil'
  endif
  //
  headmsg '1.8.6.1' 38
  if counttype '0xf7a' 'any' 'backpack' < 10
    headmsg '1.8.6' 38
    pushlist 'Regcount' 'buyregs-Black Pearl'
  endif
  //
  headmsg '1.8.7.1' 38
  if counttype '0xf7b' 'any' 'backpack' < 10
    headmsg '1.8.7' 38
    pushlist 'Regcount' 'buyregs-Blood Moss'
  endif
  //
  headmsg '1.8.8.1' 38
  if counttype '0xf8c' 'any' 'backpack' < 10
    headmsg '1.8.8' 38
    pushlist 'Regcount' 'buyregs-Sulfurous A'
  endif
  //
  headmsg '1.8.9.1' 38
  if counttype '0xf88' 'any' 'backpack' < 10
    headmsg '1.8.9' 38
    pushlist 'Regcount' 'buyregs-Nightshade'
  endif
  //
  headmsg '1.8.12' 38
  buy 'nothing'
  pause 333
  //
  headmsg '1.8.1.1'
  if @inlist! 'Regcount' 'buyregs-Mandrakeroot'
    if not inrange 'Regvendor' 15
      headmsg '1.8.1'
      pause 333
      headmsg 'Recalling to your RegVendor3'
      @canceltarget
      cast "Recall"
      waitfortarget 5000
      target! 'regvendorrune'
      pause 2000
    endif
    headmsg '1.8.2' 38
    buy '50Mandrakeroot'
    msg 'Vendor Buy Guards'
    pause 1000
  endif
  headmsg '1.8.1.2'
  if @inlist! 'Regcount' 'buyregs-Garlic'
    if not inrange 'Regvendor' 15
      headmsg '1.3.15'
      pause 333
      headmsg 'Recalling to your RegVendor4'
      @canceltarget
      cast "Recall"
      waitfortarget 5000
      target! 'regvendorrune'
      pause 2000
    endif
    headmsg '1.8.3' 38
    buy '50Garlic'
    msg 'Vendor Buy Guards'
    pause 1000
  endif
  headmsg '1.8.1.3'
  if @inlist! 'Regcount' 'buyregs-Ginseng'
    if not inrange 'Regvendor' 15
      headmsg '1.3.15'
      pause 333
      headmsg 'Recalling to your RegVendor5'
      @canceltarget
      cast "Recall"
      waitfortarget 5000
      target! 'regvendorrune'
      pause 2000
    endif
    headmsg '1.8.4' 38
    buy '50Ginseng'
    msg 'Vendor Buy Guards'
    pause 1000
  endif
  headmsg '1.8.1.4'
  if @inlist! 'Regcount' 'buyregs-Spiders Sil'
    if not inrange 'Regvendor' 15
      headmsg '1.3.15'
      pause 333
      headmsg 'Recalling to your RegVendor6'
      @canceltarget
      cast "Recall"
      waitfortarget 5000
      target! 'regvendorrune'
      pause 2000
    endif
    headmsg '1.8.5' 38
    buy '50Spiders Silk'
    msg 'Vendor Buy Guards'
    pause 1000
  endif
  headmsg '1.8.1.5'
  if @inlist! 'Regcount' 'buyregs-Black Pearl'
    if not inrange 'Regvendor' 15
      headmsg '1.3.15'
      pause 333
      headmsg 'Recalling to your RegVendor7'
      @canceltarget
      cast "Recall"
      waitfortarget 5000
      target! 'regvendorrune'
      pause 2000
    endif
    headmsg '1.8.6' 38
    buy '50Black Pearl'
    msg 'Vendor Buy Guards'
    pause 1000
  endif
  headmsg '1.8.1.6'
  if @inlist! 'Regcount' 'buyregs-Blood Moss'
    if not inrange 'Regvendor' 15
      headmsg '1.3.15'
      pause 333
      headmsg 'Recalling to your RegVendor8'
      @canceltarget
      cast "Recall"
      waitfortarget 5000
      target! 'regvendorrune'
      pause 2000
    endif
    headmsg '1.8.7' 38
    buy '50Blood Moss'
    msg 'Vendor Buy Guards'
    pause 1000
  endif
  headmsg '1.8.1.7'
  if @inlist! 'Regcount' 'buyregs-Sulfurous A'
    if not inrange 'Regvendor' 15
      headmsg '1.3.15'
      pause 333
      headmsg 'Recalling to your RegVendor9'
      @canceltarget
      cast "Recall"
      waitfortarget 5000
      target! 'regvendorrune'
      pause 2000
    endif
    headmsg '1.8.8' 38
    buy '50Sulfurous Ash'
    msg 'Vendor Buy Guards'
    pause 1000
  endif
  headmsg '1.8.1.8'
  if @inlist! 'Regcount' 'buyregs-Nightshade'
    if not inrange 'Regvendor' 15
      headmsg '1.3.15'
      pause 333
      headmsg 'Recalling to your RegVendor10'
      @canceltarget
      cast "Recall"
      waitfortarget 5000
      target! 'regvendorrune'
      pause 2000
    endif
    headmsg '1.8.9' 38
    buy '50Nightshade'
    msg 'Vendor Buy Guards'
    pause 1000
  endif
  headmsg '1.8.1.9'
  @clearlist 'Regcount'
  headmsg '1.8.12' 38
  buy 'nothing'
  pause 1000
  //
  //----------------------------------------------
  // This section Buys Regs
  //----------------------------------------------
  //
  // <-- I am here DLS
  //
  //----------------------------------------------
  // This section Gets the buff
  //----------------------------------------------
  //
  if not buffexists 'Arcane Empowerment'
    //
    // Walk 2 spaces away from dummy before recalling to buff
    //
    msg 'All Stop'
    pause 333
    walk "South"
    walk "South"
    headmsg '1.5.8'
    headmsg 'Recalling to your Bird Spot1'
    @canceltarget
    cast "Recall"
    waitfortarget 5000
    target! 'birdrune'
    pause 2000
    //
    while not buffexists 'Arcane Empowerment'
      if findtype! 0x15 30 or inrange Regvendor 15
        headmsg 'Recalling to your Bird Spot2'
        @canceltarget
        cast "Recall"
        waitfortarget 5000
        target! 'birdrune'
        pause 2000
      endif
      if inrange 'Regvendor' 15
        headmsg 'Recalling to your Bird Spot3'
        @canceltarget
        cast "Recall"
        waitfortarget 5000
        target! 'birdrune'
        pause 2000
      endif
      sysmsg "Looking for a bird to kill"
      removelist 'birds'
      createlist 'birds'
      pushlist 'birds' '0x6'
      @canceltarget
      cast 'Lightning'
      waitfortarget 5000
      while @targetexists
        for 0 to 'birds'
          if @findtype birds[] 'any' 'ground' 12
            target! 'found'
            @ignoreobject 'found'
          endif
          pause 100
        endfor
        if findobject '0x26d63' 'any' 50
          headmsg 'Recalling to the Birds'
          @canceltarget
          cast "Recall"
          waitfortarget 5000
          target! 'birdrune'
          pause 2000
        endif
      endwhile
      pause 2000
    endwhile
  endif
  //
  //----------------------------------------------
  // This section Gets the buff
  //----------------------------------------------
  //
  //
  //----------------------------------------------
  // This section Trains Magery
  //----------------------------------------------
  //
  // if not findtype! 0x15 'any' '15'
  //if not findobject (serial) [color] [source] [amount] [range]
  if not findobject '0x26d63' 'any' 50
    headmsg 'Recalling to the Zoo'
    @canceltarget
    cast "Recall"
    waitfortarget 5000
    target! 'zoorune'
    pause 2000
  else
    if skill 'Magery' >= '50' and skill 'Magery' < '100'
      if skill 'Magery' >= 50 and skill 'Magery' <= 59.9
        if hits > 50
          cast 'Lightning'
          waitfortarget 3000
          target 'self'
          pause 1000
        else
          cast 'Greater Heal'
          waitfortarget 3000
          target 'self'
          pause 1000
          cast 'Greater Heal'
          waitfortarget 3000
          target 'self'
          pause 1000
        endif
      endif
      if skill 'Magery' >= 60 and skill 'Magery' <= 69.9
        if counttype 0xf7b 0 backpack >= 1
          if counttype 0xf88 0 backpack >= 1
            cast 'Invisibility'
            waitfortarget 3000
            target 'self'
            pause 1000
          endif
        endif
      endif
      if skill 'Magery' >= 70 and skill 'Magery' <= 79.9
        if hits > 50
          cast 'Flame Strike'
          waitfortarget 3000
          target 'self'
          pause 1000
        else
          cast 'Greater Heal'
          waitfortarget 3000
          target 'self'
          pause 1000
          cast 'Greater Heal'
          waitfortarget 3000
          target 'self'
          pause 1000
        endif
      endif
      if skill 'Magery' >= 80 and skill 'Magery' <= 89.9
        sysmsg 'magery over 80' 38
        if hits > 50
          //cast 'Flame Strike'
          cast "Summon Water Elemental"
          // waitfortarget 3000
          // targettype! 0x15
          pause 333
          if followers > 3
            msg 'all release'
            waitforgump 0x3780b38e 15000
            replygump 0x3780b38e 1
            pause 333
          endif
        else
          cast 'Greater Heal'
          waitfortarget 3000
          target 'self'
          pause 1000
          cast 'Greater Heal'
          waitfortarget 3000
          target 'self'
          pause 1000
        endif
      endif
    endif
  endif
  //
  //----------------------------------------------
  // This section Trains Magery
  //----------------------------------------------
  //
  //
  //----------------------------------------------
  // This section Trains Evaluating Intelligence
  //----------------------------------------------
  //
  if skill 'Evaluating Intelligence' >= '50' and skill 'Evaluating Intelligence' < '100'
    getfriend 'any'
    setalias 'evaltarget' 'found'
    pause 333
    useskill 'Evaluating Intelligence'
    waitfortarget 2000
    target! 'evaltarget'
    pause 2000
  endif
  //
  //----------------------------------------------
  // This section Trains Evaluating Intelligence
  //----------------------------------------------
  //
  //
  //----------------------------------------------
  // This section Trains Spirit Speak
  //----------------------------------------------
  //
  if skill 'Spirit Speak' >= '50' and skill 'Spirit Speak' < '100'
    useskill 'Spirit Speak'
    pause 2000
  endif
  if mana < 30
    while mana < 90
      useskill 'meditation'
      pause 6000
    endwhile
  endif
  //
  //----------------------------------------------
  // This section Trains Spirit Speak
  //----------------------------------------------
  //
  //
  //----------------------------------------------
  // This section is the end of the script
  //----------------------------------------------
  //
endwhile
//
//----------------------------------------------
// This section is the end of the script
//----------------------------------------------
//
