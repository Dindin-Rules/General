//
clearlist 'Recallstatus'
@createlist 'Recallstatus'
//
//
clearlist 'Buffs'
@createlist 'Buffs'
//
clearlist 'Regcount'
@createlist 'Regcount'
//
//
clearlist 'Regcountpassattempts'
@createlist 'Regcountpassattempts'
//
clearlist 'vendoractions'
@createlist 'vendoractions'
//
clearlist 'StopAction'
@createlist 'StopAction'
//
@removelist 'scriptregs'
@createlist 'scriptregs'
pushlist 'scriptregs' 0xf7a // pearl
pushlist 'scriptregs' 0xf7b // moss
pushlist 'scriptregs' 0xf86 // root
pushlist 'scriptregs' 0xf8c // ash
pushlist 'scriptregs' 0xf88 // Nightshade
pushlist 'scriptregs' 0xf85 // Ginseng
pushlist 'scriptregs' 0xf8d // Spiders Silk
pushlist 'scriptregs' 0xf84 // Garlic
//
//
@removelist 'Vendorrunes'
@createlist 'Vendorrunes'
pushlist 'Vendorrunes' 'regvendor '
pushlist 'Vendorrunes' 'regvendor2'
