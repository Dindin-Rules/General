headmsg '1.2 (around line 380) Lists ' 38
clearlist 'status'
@createlist 'status'
pushlist 'status' 'running'
//
clearlist 'setup'
@createlist 'setup'
//
clearlist 'actions'
@createlist 'actions'
//
clearlist 'Backpackstatus'
